{
  "expo": {
    "name": "Nexfolio",
    "slug": "nexfolio",
    "scheme": "nexfolio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-1763608570256.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "your-eas-project-id"
      },
      "EXPO_PUBLIC_BACKEND_URL": "https://nexfolio.onrender.com"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nexfolio.app"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-1763608570256.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nexfolio.app"
    },
    "plugins": [
      "expo-asset",
      "expo-build-properties",
      "expo-font",
      "expo-mail-composer",
      "expo-secure-store",
      "expo-sqlite",
      "expo-video",
      "expo-web-browser"
    ]
  }
}
